@charset "utf-8";
#wrap{
    width:100%;
    height:auto;
}
.header{
    width:100%;
    height:auto;
    border:1px solid;   
    box-sizing:border-box;
    background:#ff0;
}
.contents{
    width:100%;
    max-width:700px;
    height:auto;
    border:0px solid #f00;
    margin:0 auto;
}
.footer{
    width:100%;
    max-width:700px;
    height:200px;
    background:#000;
    margin:0 auto;
  
}
/*header 시작*/
.header .gnb{
    width:100%;
    max-width:700px;
    height:auto;
    overflow:hidden;
    background:#ccc;
    margin:0 auto;
}
.header .gnb>ul>li{
    border:1px solid ;
    box-sizing:border-box;
    line-height:60px;
    text-align:center;
    float:left;
}
.header .gnb>ul>li.one{
/*     width:140px; */
    width:20%;
    font-size:30px;
}
.header .gnb>ul>li.two{
    width:14%;
/*     width:100px; */
    font-size:14px;
}
.header .gnb>ul>li.three{
    width:12%;
/*     width:80px; */
    font-size:10px;
}
.header .gnb>ul>li a{
    background:#00f;
}
.header .gnb>ul>li a:hover{
    color:#f00;
}
.header .gnb>ul>li.two >a{
    display:block;
    width:60%;
    color:#fff;
    height:40px;
    line-height:40px;
    margin-left:20%;
    margin-top:10px;
    -ms-transition:all 0.5s;
    -moz-transition:all 0.5s;
    -webkit-transition:all 0.5s;
    transition:all 0.5s;
}
.header .gnb>ul>li.two >a:hover{
    color:#f00;
    background:#ff0;
    border-radius:50%;
}
/*contents 시작*/
.contents .section{
    width:100%;
    height:500px;
    border:10px solid #d8c2d8;
    box-sizing:border-box;
}


/*footer 시작*/