@charset "UTF-8";

#wrap {
  width : 600px;
  height : auto;
  background: #999;
  margin : 0 auto;
  text-align: center;
  font-size: 35px;
  font-weight: bold;
  color : #fbffff;
  border : 20px solid lightgray;
  border-radius: 40px;

}

#wrap header{
  width : 100%;
  height : 100px;
  background : #fd0;
  line-height: 100px;
  display: inline-block;

}

nav {
  width : 100%;
  height : 100px;
  background : #ced;
  line-height: 100px;
}

section.contents {
  width : 100%;
  height: 400px;
  background : #aa0;
  position: relative; /*중심 포지션*/
}

section.top {
  width : 400px;
  height : 200px;
  background : #dd9
  ;
  position: relative;
  line-height: 200px;
}

article {
  width : 400px;
  height: 200px;
  background : #5aa;
  line-height: 200px;
}
aside {
  width: 200px;
  height: 400px;;
  background: #9dd;
  position: absolute;
  right : 0px;
  top : 0px;
  line-height: 400px;
}
footer {
  width : 100%;
  height : 100px;
  line-height: 100px;
}
