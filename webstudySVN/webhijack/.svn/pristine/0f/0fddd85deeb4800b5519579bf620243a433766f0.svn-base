@charset "utf-8";
#wrap{
    width:100%;
    height:auto;
  
    background-image: url(../images/E-Mail.png);
    background-size:50px;
}

.section{
    max-width:1200px;
    height:auto;
  
    margin: 0 auto;
    overflow:hidden;   
}

.footer{
    max-width:1200px;
    height:auto;
    border:10px solid brown;
    margin: 0 auto;
    overflow:hidden;
}

/*section 시작*/

.section.sec1{
    
    height:auto;
    margin:0 auto;
}

.section.sec1>ul>li{
    width:100%;
    height:600px;
    border:10px solid skyblue;
    box-sizing:border-box;
    font-size:100px;
    color:white;
}

.section.sec1>ul>li:nth-child(1){
    height: 1200px;
    background-color:rgba(0,0,0,0.3);
    background-image: url(../images/bg0.jpg);
    background-repeat:no-repeat;
    background-size:cover; 
    background-position:50% 0px;
    
}

.section.sec1>ul>li:nth-child(2){
    background-color:rgba(0,0,0,0.3);
    background-image: url(../images/bg1.jpg);
    background-repeat:no-repeat;
    background-size:cover; 
    background-position:50% 0px;
    
}

.section.sec1>ul>li:nth-child(3){
    background-color:rgba(0,0,0,0.3);
    background-image: url(../images/bg2.jpg);
    background-repeat:no-repeat;
    background-size:cover; 
    background-position:50% 0px;}


/* .section.sec2 */

