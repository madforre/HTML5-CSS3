@charset "utf-8";


.nav {
    width:100%; height:auto;
/*background: powderblue;*/
}
.nav .gnb {
    width:680px; height:47px; 
    background-color: aliceblue;
    margin: 0 auto;  
}
.nav .gnb .test{
    margin-left : 5px;
}

.nav .gnb li{    
    height:32px;
    border : 1px solid gray;
    box-sizing: border-box;
    margin: 8px 10.5px 0 0;
    background-color: #fff;
    font-weight: bold;
    line-height: 30px;
    float:left; 
}

.nav .gnb .green{
    font-size: 15px;
    color: yellowgreen;
}
.nav .gnb .black{
    font-size: 13px;
    color: black;
}
